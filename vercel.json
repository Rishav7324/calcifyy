{
    "builds": [
        {
            "src": "vite.config.ts",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/sitemap.xml",
            "dest": "/sitemap.xml"
        },
        {
            "src": "/robots.txt",
            "dest": "/robots.txt"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}
